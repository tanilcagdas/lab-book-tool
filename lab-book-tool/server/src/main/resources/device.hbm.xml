<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.labbooktool.model.Device" table="DEVICES">
	   <id name="id" type="int" column="ID" >
	   <generator class="assigned"/>
	  </id>
	  
	  <property name="ip">
		 <column name="IP" />
		 </property>
      <property name="version">
		 <column name="VERSION" />
		 </property>
	  <property name="product">
		 <column name="PRODUCT" />
		 </property>

	  <property name="name">
		 <column name="NAME" />
	  </property>
	  <property name="status">
		<column name="STATUS"/>
	  </property>

   </class>
   
     <class name="com.labbooktool.model.Laptop" table="LAPTOPS">
	   <id name="id" type="int" column="ID" >
	   <generator class="assigned"/>
	  </id>
	  
	  <property name="password">
		 <column name="PASSWORD" />
		 </property>
      <property name="os">
		 <column name="OS" />
		 </property>
	  <property name="name">
		 <column name="NAME" />
	  </property>
	  <property name="status">
		<column name="STATUS"/>
	  </property>

   </class>





</hibernate-mapping>